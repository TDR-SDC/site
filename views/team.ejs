            <!-- <div id="menu_section"> -->
                <ul class="d-f-r ai-c jc-c" id="menu_list" style="padding: 0;">
                    <div onclick="team()">
                        <li class="d-f ai-c jc-c" style="visibility: visible;">Current Team</li>
                    </div>
                    <div onclick="alumni()">
