                    <span class="dept_heading">Management</span>
                    <span class="dept_description">
                        The conscience
                    </span>

                    <span class="members_list d-f-r jc-c ai-c">
                        <% users.forEach(function(user){ %>
                            <% if(user.management==true) {%>
                                <div class="member d-f-c jc-c ai-c">
                                    <% if(user.avatar) {%>
                                        <img src="<%= user.avatar %>">
                                        <%} else {%>
                                            <img src="/imgs/team_members/placeholder.png">
                                            <% } %>
                                                <div class="d-f-c jc-sb ai-c">
                                                    <span class="member_name">
                                                        <%= user.name %>
                                                    </span>
                                                    <span class="member_position">
                                                        <%= user.position %>
                                                    </span>
                                                    <span class="member_social">
                                                        <% if(user.social.insta) {%>
                                                            <a href="<%= user.social.insta %>" target="_blank"><img
                                                                    src="/imgs/icons/instagram.svg"></a>
                                                            <% } %>
                                                                <% if(user.social.linkedin) {%>
                                                                    <a href="<%= user.social.linkedin %>"
                                                                        target="_blank"><img
                                                                            src="/imgs/icons/linkedin.svg"></a>
                                                                    <% } %>
