            <!-- Alumni List (open to all) -->
            <div class="content" id="alumni">
                <div class="table">
                    <div class="table-header">
                        <div style="flex: 5;" class="header__item">
                            <span class="filter__link">Name</span>
                        </div>
                        <div style="flex: 5;" class="header__item">
                            <span class="filter__link filter__link--number">Department</span>
                        </div>
                        <div style="flex: 1;" class="header__item">
                            <span class="filter__link filter__link--number"></span>
                        </div>
                    </div>
                    <div class="table-content">
                        <% alumni.forEach(function(alum) {%>
                            <span class="table-row">
                                <div style="flex: 5;" class="table-data">
                                    <%= alum.name %>
                                </div>
                                <div style="flex: 5; text-transform: capitalize;" class="table-data">
                                    <%= alum.dept %>
                                </div>
                                <div style="flex: 1;" class="table-data">
                                    <% if(alum.linkedin) {%>
                                    <a class="alum_linkedin" href="<%= alum.linkedin %>">
                                        <img src="/imgs/icons/linkedin.svg">
                                    </a>
                                    <% } %>
